<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="NativeAdAttribution">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">12sp</item>
        <item name="android:text">AD</item>
        <item name="android:background">@color/exit_native_ad_text_bg_color</item>
        <item name="android:layout_marginLeft">@dimen/_2sdp</item>
        <item name="android:layout_marginTop">@dimen/_2sdp</item>
        <item name="android:paddingLeft">@dimen/_2sdp</item>
        <item name="android:paddingRight">@dimen/_2sdp</item>
        <item name="android:paddingTop">@dimen/_2sdp</item>
        <item name="android:paddingBottom">@dimen/_2sdp</item>
    </style>

    <style name="ExitNativeAdRatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">@color/exit_native_rating_empty_color</item>
        <item name="colorControlActivated">@color/exit_native_rating_fill_color</item>
    </style>
</resources>